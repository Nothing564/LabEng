<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Livro.LivroVO" table="livro">
        <id name="cod" type="integer">
            <column name="CodLivro"/>
        </id>
        <property name="isbn" type="float">
      <column length="45" name="isbn" not-null="true"/>
    </property>
    <property name="autor" type="string">
      <column length="45" name="autor" not-null="true"/>
    </property>
    <property name="categoria" type="string">
      <column length="45" name="categoria" not-null="true"/>
    </property>
    <property name="titulo" type="string">
      <column length="45" name="titulo" not-null="true"/>
    </property>
    <property name="precoVenda" type="double">
      <column length="45" name="ValorUnt" not-null="true"/>
    </property>
    <property name="quantidade" type="integer">
      <column length="45" name="qtd" not-null="true"/>
    </property>
    </class>
  
</hibernate-mapping>
